{
    "name": "ivancraft623/netherreactor",
    "description": "Plugin for PocketMine-MP that implements the nether reactor",
    "type": "project",
    "license": "LGPL-3.0",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "customiesdevs/customies",
				"version": "1.3.2",
				"source": {
					"url": "https://github.com/CustomiesDevs/Customies.git",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"psr-4": {
						"customiesdevs\\customies\\": "src/"
					}
				}
			}
